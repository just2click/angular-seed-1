{
  "name": "angular2-seed",
  "version": "0.9.0",
  "description": "Angular 2 seed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meeroslav/angular2-seed.git"
  },
  "keywords": [
    "angular2",
    "seed",
    "webpack",
    "typescript",
    "SPA",
    "ng2-translate"
  ],
  "author": "Miroslav Jonas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meeroslav/angular2-seed/issues"
  },
  "homepage": "https://github.com/meeroslav/angular2-seed#readme",
  "scripts": {
    "clean": "rimraf coverage dist doc",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "build:prod": "set NODE_ENV=production && npm run build",
    "watch": "webpack --watch --progress --profile",
    "test": "rimraf coverage && karma start --single-run",
    "test:watch": "karma start --no-single-run --auto-watch",
    "posttest": "remap-istanbul -i coverage/report.json -o coverage/html-report -t html && remap-istanbul -i coverage/report.json -o coverage/report.txt -t teamcity",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "protractor": "protractor",
    "e2e": "protractor config/protractor.conf.js",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "docs": "typedoc --options typedoc.json src/",
    "start": "webpack-dashboard -- webpack-dev-server --historyApiFallback --port 51961"
  },
  "dependencies": {
    "@angular/common": "2.4.6",
    "@angular/compiler": "2.4.6",
    "@angular/core": "2.4.6",
    "@angular/forms": "2.4.6",
    "@angular/http": "2.4.6",
    "@angular/platform-browser": "2.4.6",
    "@angular/platform-browser-dynamic": "2.4.6",
    "@angular/router": "3.4.6",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "ng2-bootstrap": "1.3.3",
    "ng2-translate": "^5.0.0",
    "ngx-modal-dialog": "^0.5.3",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.1.0",
    "zone.js": "0.7.6"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.41",
    "@types/node": "7.0.5",
    "@types/selenium-webdriver": "^2.53.39",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "2.0.0",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "0.6.0",
    "autoprefixer": "^6.3.2",
    "awesome-typescript-loader": "^2.2.4",
    "copy-webpack-plugin": "^4.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.8.1",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^2.4.0",
    "json-loader": "^0.5.3",
    "karma": "^1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-remap-istanbul": "^0.5.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.0.0",
    "karma-webpack": "2.0.2",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "null-loader": "0.1.1",
    "phantomjs-polyfill": "0.0.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^1.2.2",
    "protractor": "^4.0.10",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.9.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.5",
    "typescript": "2.0.10",
    "webpack": "~2.2.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "^2.3.0"
  }
}
