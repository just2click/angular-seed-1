{
  "name": "angular-seed",
  "version": "1.0.0",
  "description": "Custom angular seed for Greentube projects ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meeroslav/angular-seed.git"
  },
  "keywords": [
    "angular",
    "seed",
    "webpack",
    "typescript",
    "SPA",
    "ngx-translate",
    "ngx-bootstrap"
  ],
  "author": "Miroslav Jonas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meeroslav/angular-seed/issues"
  },
  "homepage": "https://github.com/meeroslav/angular-seed#readme",
  "scripts": {
    "build": "rimraf dist && webpack --progress --profile --bail",
    "build:prod": "set NODE_ENV=production && npm run build",
    "test": "rimraf coverage && karma start --single-run",
    "test:watch": "karma start --no-single-run --auto-watch",
    "posttest": "remap-istanbul -i coverage/report.json -o coverage/html-report -t html && remap-istanbul -i coverage/report.json -o coverage/report.txt -t teamcity",
    "protractor": "protractor",
    "e2e": "protractor config/protractor.conf.js",
    "e2e:local": "protractor .config/protractor.conf.js",
    "e2e:lserver": "npm-run-all -p -r server:run e2e:local",
    "server:run": "lite-server",
    "docs": "typedoc --options typedoc.json src/",
    "start": "webpack-dashboard -- webpack-dev-server --watch-poll --port 51961",
    "postinstall": "node ./node_modules/protractor/bin/webdriver-manager update"
  },
  "dependencies": {
    "@angular/animations": "~5.0.1",
    "@angular/common": "~5.0.1",
    "@angular/compiler": "~5.0.1",
    "@angular/core": "~5.0.1",
    "@angular/forms": "~5.0.1",
    "@angular/http": "~5.0.1",
    "@angular/platform-browser": "~5.0.1",
    "@angular/platform-browser-dynamic": "~5.0.1",
    "@angular/router": "~5.0.1",
    "@angular/language-service": "~5.0.1",
    "@ngx-translate/core": "~8.0.0",
    "@ngx-translate/http-loader": "~2.0.0",
    "@swimlane/ngx-datatable": "~11.0.3",
    "angular2-virtual-scroll": "~0.3.1",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "~2.5.1",
    "ngx-bootstrap": "2.0.0-beta.8",
    "ngx-modal-dialog": "~1.0.0",
    "ramda": "~0.25.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.2.0",
    "zone.js": "0.8.12"
  },
  "devDependencies": {
    "@types/hammerjs": "~2.0.35",
    "@types/jasmine": "~2.6.3",
    "@types/jasmine-data_driven_tests": "~1.0.1",
    "@types/node": "~8.0.50",
    "@types/ramda": "~0.25.3",
    "@types/selenium-webdriver": "~3.0.8",
    "@types/source-map": "~0.5.2",
    "@types/uglify-js": "~2.6.29",
    "@types/webpack": "~3.8.1",
    "angular-router-loader": "~0.6.0",
    "angular2-template-loader": "~0.6.2",
    "autoprefixer": "~7.1.6",
    "awesome-typescript-loader": "~3.3.0",
    "chalk": "^2.3.0",
    "codelyzer": "4.0.1",
    "copy-webpack-plugin-advanced": "~1.0.3",
    "css-loader": "~0.28.7",
    "extract-text-webpack-plugin": "~3.0.2",
    "faker": "~4.1.0",
    "file-loader": "~1.1.5",
    "html-webpack-plugin": "~2.30.1",
    "istanbul-instrumenter-loader": "~3.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-data_driven_tests": "~1.0.0",
    "jasmine-reporters": "~2.2.1",
    "jasmine-spec-reporter": "~4.1.0",
    "json-loader": "~0.5.7",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "~0.2.2",
    "karma-mocha-reporter": "~2.2.5",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-teamcity-reporter": "~1.0.1",
    "karma-webpack": "~2.0.5",
    "lite-server": "~2.3.0",
    "lodash": "^4.17.4",
    "node-sass": "~4.6.0",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.2.2",
    "protractor": "~5.2.0",
    "protractor-jasmine2-screenshot-reporter": "~0.5.0",
    "raw-loader": "0.5.1",
    "remap-istanbul": "~0.9.5",
    "rimraf": "^2.5.1",
    "sass-loader": "~6.0.6",
    "selenium-webdriver": "~3.5.0",
    "style-loader": "~0.19.0",
    "ts-helpers": "~1.1.2",
    "ts-node": "~3.3.0",
    "tslint": "5.8.0",
    "tslint-eslint-rules": "~4.1.1",
    "tslint-loader": "3.5.3",
    "typedoc": "~0.9.0",
    "typescript": "2.5.3",
    "webpack": "~3.8.1",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "~2.9.4"
  }
}
