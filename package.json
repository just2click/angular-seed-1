{
  "name": "angular2-seed",
  "version": "1.0.0",
  "description": "Angular seed",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meeroslav/angular2-seed.git"
  },
  "keywords": [
    "angular",
    "seed",
    "webpack",
    "typescript",
    "SPA",
    "ngx-translate",
    "ngx-bootstrap"
  ],
  "author": "Miroslav Jonas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meeroslav/angular2-seed/issues"
  },
  "homepage": "https://github.com/meeroslav/angular2-seed#readme",
  "scripts": {
    "clean": "rimraf coverage dist doc",
    "build": "rimraf dist && webpack --progress --profile --bail",
    "build:prod": "set NODE_ENV=production && npm run build",
    "watch": "webpack --watch --progress --profile",
    "test": "rimraf coverage && karma start --single-run",
    "test:watch": "karma start --no-single-run --auto-watch",
    "posttest": "remap-istanbul -i coverage/report.json -o coverage/html-report -t html && remap-istanbul -i coverage/report.json -o coverage/report.txt -t teamcity",
    "webdriver:update": "webdriver-manager update",
    "webdriver:start": "webdriver-manager start",
    "protractor": "protractor",
    "e2e": "protractor config/protractor.conf.js",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "docs": "typedoc --options typedoc.json src/",
    "start": "webpack-dashboard -- webpack-dev-server --historyApiFallback --port 51961"
  },
  "dependencies": {
    "@angular/animations": "^4.2.6",
    "@angular/common": "^4.2.6",
    "@angular/compiler": "^4.2.6",
    "@angular/core": "^4.2.6",
    "@angular/forms": "^4.2.6",
    "@angular/http": "^4.2.6",
    "@angular/platform-browser": "^4.2.6",
    "@angular/platform-browser-dynamic": "^4.2.6",
    "@angular/router": "^4.2.6",
    "@ngx-translate/core": "^7.0.0",
    "@ngx-translate/http-loader": "^0.1.0",
    "@swimlane/ngx-datatable": "^9.3.1",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "ngx-bootstrap": "1.7.1",
    "ngx-modal-dialog": "^1.0.0",
    "ramda": "^0.23.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.45",
    "@types/jasmine-data_driven_tests": "^1.0.1",
    "@types/node": "~6.0.60",
    "@types/ramda": "0.0.5",
    "@types/selenium-webdriver": "^2.53.39",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "2.0.0",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "0.6.2",
    "autoprefixer": "^6.3.2",
    "awesome-typescript-loader": "^3.1.3",
    "copy-webpack-plugin-advanced": "1.0.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.8.1",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "~2.6.2",
    "jasmine-data_driven_tests": "^1.0.0",
    "jasmine-spec-reporter": "~4.1.0",
    "json-loader": "^0.5.4",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-teamcity-reporter": "^1.0.0",
    "karma-webpack": "2.0.2",
    "less": "^2.7.1",
    "less-loader": "^4.0.2",
    "lodash": "^4.17.4",
    "node-sass": "^4.5.0",
    "null-loader": "0.1.1",
    "postcss-loader": "^1.2.2",
    "protractor": "^4.0.10",
    "raw-loader": "0.5.1",
    "remap-istanbul": "^0.9.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.1",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "tslint-loader": "^3.3.0",
    "typedoc": "^0.5.5",
    "typescript": "~2.3.3",
    "webpack": "~2.4.0",
    "webpack-dashboard": "^0.3.0",
    "webpack-dev-server": "~2.4.5"
  }
}
